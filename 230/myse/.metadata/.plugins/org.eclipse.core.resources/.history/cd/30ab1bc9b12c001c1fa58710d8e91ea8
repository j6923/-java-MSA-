/**
 * 고객 객체용 클래스이다. 
 * 고객정보(아이디, 이름, 비밀번호, 주소 )
 * @author KOSTA
 * 
 */
class Customer{
	String id;
	String pwd;
	String name;
	String address;
	
	Customer(){}
	/**
	 * 고객정보를 초기화한다. 
	 * @param id 아이디
	 * @param pwd 비밀번호
	 * @param name 이름
	 * @param address 주소 
	 */
	Customer(String id, String pwd, String name, String address){
		this.id = id;
		this.pwd = pwd;
		this.name = name;
		this.address = address;
	}
	void printInfo() {
		System.out.println("아이디:" + id + ", 비밀번호: " + pwd + ",이름:" + name + ",주소: " + address);
	}
	
	
	
}

